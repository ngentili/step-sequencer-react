{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "firefox",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}",
            "profile": "dev-edition-default",
        }
    ]
}